{
  "extends": "@istanbuljs/nyc-config-babel",
  "all": true,
  "check-coverage": true,
  "include": ["src/**/*.js"],
  "thresholds": {
    "branches": 98,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  }
}
